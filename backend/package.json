{
  "name": "snap-site-api",
  "version": "1.0.0",
  "description": "REST API for Snap Site",
  "main": "src/index.ts",
  "author": "Aadarsh Jha",
  "scripts": {
    "start": "npm run build && pm2 start ecosystem.config.json --no-daemon",
    "dev": "nodemon -r tsconfig-paths/register -r ts-node src/index.ts",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "docker:dev-db:start": "docker-compose -f docker-compose.only-db-dev.yml up -d",
    "docker:dev-db:stop": "docker-compose -f docker-compose.only-db-dev.yml down",
    "build": "rimraf build && tsc -p tsconfig.json && tscpaths -p tsconfig.json -s . -o ./build",
    "cz": "cz",
    "jobs:index": "nodemon -r tsconfig-paths/register -r ts-node src/jobs/index.ts",
    "jobs:rescrape": "nodemon -r tsconfig-paths/register -r ts-node src/jobs/rescrape.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@jest/globals": "^29.6.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/user-agents": "^1.0.4",
    "@types/uuid": "^9.0.7",
    "@types/xss-filters": "^0.0.27",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "commitizen": "^4.3.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.0",
    "node-mocks-http": "^1.13.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.484.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@aws-sdk/types": "^3.468.0",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.1",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "fs": "^0.0.1-security",
    "helmet": "^7.0.0",
    "http-status": "^1.6.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.4",
    "pm2": "^5.3.0",
    "puppeteer": "^21.7.0",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^4.6.12",
    "user-agents": "^1.1.87",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-filters": "^1.2.7",
    "zod": "^3.22.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
